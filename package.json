{
  "name": "nextjs-shop",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "migration": "npm run migration:create:db && npm run migration:up",
    "migration:init": "npx sequelize-cli init",
    "migration:create:db": "sequelize db:create",
    "migration:up": "npx sequelize-cli db:migrate up",
    "generateHoocs": "graphql-codegen",
    "clearHoocs": "find ./components -name \"__generated__\" -exec rm -rf '{}' +"
  },
  "dependencies": {
    "@apollo/react-hooks": "^4.0.0",
    "@apollo/react-ssr": "^4.0.0",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "apollo-server-micro": "^2.18.1",
    "dotenv": "^8.2.0",
    "graphql": "^15.3.0",
    "micro-cors": "^0.1.1",
    "mysql2": "^2.2.5",
    "next": "9.5.3",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "sass": "^1.26.11",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.17.10",
    "@graphql-codegen/near-operation-file-preset": "^1.17.11",
    "@graphql-codegen/typescript": "^1.17.10",
    "@graphql-codegen/typescript-operations": "^1.17.8",
    "@graphql-codegen/typescript-react-apollo": "^2.0.7",
    "@types/node": "^14.11.2",
    "@types/react": "^16.9.50",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-standard": "^4.0.1",
    "graphql-codegen": "^0.4.0",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  }
}
